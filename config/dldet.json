{
    "description": {
        "preset_name_en": "DLDet",
        "preset_name_cn": "DLDet",
        "intro_cn": "轻量级深度学习检测器，适合有充足算力的场合。",
        "intro_en": "Lightweight deep-learning detector. Suitable when having enough computing power."
    },
    "loader": {
        "name": "ThreadVideoLoader",
        "wrapper": "PyAVVideoWrapper",
        "resize": [
            960,
            544
        ],
        "exp_time": 0.5,
        "merge_func": "max",
        "grayscale": false,
        "continue_on_err": true
    },
    "detector": {
        "name": "MLDetector",
        "window_sec": 1,
        "cfg": {
            "model": {
                "name":"YOLOModel",
                "weight_path": "./weights/yolov5s_v2.onnx",
                "dtype": "float32",
                "nms": true,
                "warmup": true,
                "pos_thre": 0.25,
                "nms_thre": 0.45,
                "multiscale_pred":1,
                "multiscale_partition":2
            }
        }
    },
    "collector": {
        "meteor_cfg": {
            "min_len": 15,
            "max_interval": 5,
            "time_range": [
                0,
                8
            ],
            "speed_range": [
                2,
                21
            ],
            "drct_range": [
                0,
                0.6
            ],
            "det_thre": 0.5,
            "thre2": 2048
        },
        "recheck_cfg": {
            "switch": true,
            "model": {
                "name":"YOLOModel",
                "weight_path": "./weights/yolov5s_v2.onnx",
                "dtype": "float32",
                "nms": true,
                "warmup": true,
                "pos_thre": 0.25,
                "nms_thre": 0.45,
                "multiscale_pred":2,
                "multiscale_partition":2
            },
            "save_path":""
        },
        "positive_cfg": {
            "positive_cates": [
                "METEOR",
                "RED_SPRITE"
            ]
        }
    }
}