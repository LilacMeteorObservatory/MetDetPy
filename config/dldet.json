{
    "description": {
        "preset_name_en": "DLDet",
        "preset_name_cn": "DLDet",
        "intro_cn": "轻量级深度学习检测器，适合有算力的场合。",
        "intro_en": "Lightweight deep-learning detector. Suitable when having spare computing power."
    },
    "loader": {
        "name": "ThreadVideoLoader",
        "wrapper": "OpenCVVideoWrapper",
        "resize": [
            960,
            544
        ],
        "exp_time": 0.5,
        "merge_func": "max",
        "grayscale": false
    },
    "detector": {
        "name": "MLDetector",
        "window_sec": 1,
        "cfg": {
            "model": {
                "name":"YOLOModel",
                "weight_path": "./weights/yolov5s.onnx",
                "dtype": "float32",
                "nms": true,
                "warmup": true,
                "pos_thre": 0.25,
                "nms_thre": 0.45
            }
        }
    },
    "collector": {
        "meteor_cfg": {
            "min_len": 15,
            "max_interval": 5,
            "time_range": [
                0,
                15
            ],
            "speed_range": [
                20,
                150
            ],
            "drct_range": [
                0,
                0.5
            ],
            "det_thre": 0.5,
            "thre2": 2048
        },
        "recheck_cfg": {
            "switch": true,
            "model": {
                "name":"YOLOModel",
                "weight_path": "./weights/yolov5s.onnx",
                "dtype": "float32",
                "nms": true,
                "warmup": true,
                "pos_thre": 0.25,
                "nms_thre": 0.45
            },
            "save_path":""
        }
    }
}