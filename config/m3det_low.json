{
    "description": {
        "preset_name_cn": "M3Det-稳健",
        "preset_name_en": "M3Det-robust",
        "intro_cn": "自适应流星检测器，适用于噪声较多的场景。",
        "intro_en": "Adaptive meteor detector. Suitable for scenarios with high noise."
    },
    "loader": {
        "name": "ThreadVideoLoader",
        "wrapper": "OpenCVVideoWrapper",
        "resize": 960,
        "exp_time": "auto",
        "merge_func": "max",
        "grayscale": true,
        "upper_bound": 0.5
    },
    "detector": {
        "name": "M3Detector",
        "window_sec": 1,
        "cfg": {
            "binary": {
                "adaptive_bi_thre": true,
                "init_value": 7,
                "sensitivity": "normal",
                "area": 0.1,
                "interval": 2
            },
            "hough_line": {
                "threshold": 10,
                "min_len": 15,
                "max_gap": 8
            },
            "dynamic": {
                "dy_mask": true,
                "dy_gap": 0.05,
                "fill_thre": 0.6
            }
        }
    },
    "collector": {
        "meteor_cfg": {
            "min_len": 20,
            "max_interval": 5,
            "time_range": [
                0,
                8
            ],
            "speed_range": [
                0.9,
                18
            ],
            "drct_range": [
                0,
                0.6
            ],
            "det_thre": 0.5,
            "thre2": 2048
        },
        "recheck_cfg": {
            "switch": true,
            "model": {
                "name":"YOLOModel",
                "weight_path": "./weights/yolov5s.onnx",
                "dtype": "float32",
                "nms": true,
                "warmup": true,
                "pos_thre": 0.25,
                "nms_thre": 0.45
            },
            "save_path":""
        }
    }
}